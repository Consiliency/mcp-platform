  "servers": [
    {
      "id": "filesystem",
      "name": "Filesystem",
      "description": "Access and manipulate files and directories on the local system",
      "version": "1.0.0",
      "category": "data",
      "source": {
        "type": "npm",
        "package": "@modelcontextprotocol/server-filesystem",
        "version": "latest"
      },
      "docker": {
        "image": null,
        "build": {
          "dockerfile": "templates/npm.Dockerfile",
          "args": {
            "PACKAGE": "@modelcontextprotocol/server-filesystem"
          }
        }
      },
      "config": {
        "port": 3001,
        "environment": {
          "MCP_MODE": "http"
        },
        "volumes": [
          "${HOME}:/workspace:ro"
        ],
        "capabilities": ["read", "write", "list"]
      },
      "dependencies": [],
      "healthCheck": {
        "path": "/health",
        "interval": "30s",
        "timeout": "5s",
        "retries": 3,
        "startPeriod": "30s"
      },
      "lifecycle": {
        "startupTimeout": "60s",
        "shutdownGracePeriod": "30s",
        "restartPolicy": "unless-stopped"
      },
      "clients": ["claude-code", "vs-code", "cursor"],
      "tags": ["filesystem", "storage", "io"]
    },
